    "babel-jest": "^27.x.x",
    "babel-loader": "^8.x.x",
    "copy-webpack-plugin": "^11.x.x",
    "css-loader": "^6.x.x",
    "cssnano": "^5.x.x",
    "file-loader": "^6.x.x",
    "html-webpack-plugin": "^5.x.x",
    "identity-obj-proxy": "^3.x.x",
    "jest": "^27.x.x",
    "mini-css-extract-plugin": "^2.x.x",
